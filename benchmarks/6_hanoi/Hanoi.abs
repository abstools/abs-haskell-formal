module Hanoi;

interface IMain {
	Int hanoi(Int a, Int b, Int tmp, Int n);
}

class Main implements IMain{
	Int hanoi(Int a, Int b, Int tmp, Int n) {
	  Int res = 0;
	  if (n > 0) {
	    Int n1 = n - 1;
      res = this.hanoi(a, tmp, b, n1);
      n1 = n - 1;
	    res = this.hanoi(tmp, b, a, n1);
	  }
	  return res;
	}
	
}

